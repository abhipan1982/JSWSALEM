    <Folder Include="Managers\MVH\" />
    <Folder Include="Managers\PPL\" />
    <Folder Include="Managers\PRF\" />
    <Folder Include="Managers\PRM\" />
    <Folder Include="Managers\QTY\" />
    <Folder Include="Managers\RLS\" />
    <Folder Include="Managers\SIM\" />
    <Folder Include="Managers\STP\" />
    <Folder Include="Managers\TRK\" />
    <Folder Include="Managers\WBF\" />
    <Folder Include="Managers\YRD\" />
    <Folder Include="Managers\ZPC\" />
  </ItemGroup>
  <ItemGroup>
