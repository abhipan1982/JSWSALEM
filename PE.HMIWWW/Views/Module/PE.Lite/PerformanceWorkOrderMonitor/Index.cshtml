@{
  Layout = "~/Views/Shared/_LayoutWithoutWidgets.cshtml";
}

<link rel="stylesheet" href="~/bundles/css/Module/PerformanceWorkOrderMonitor.css" />

<div class="performance-legend-window">
  <div class="row m-2">
    <div class="square status-isBlocked"></div><div class="legend">@VM_Resources.NAME_IsBlocked</div>
  </div>
</div>

<div class="container-fluid p-3">
  <div class="row">
    <div class="col-3 grid-filter">
      <partial name="~/Views/Module/PE.Lite/PerformanceWorkOrderMonitor/_WorkOrderMonitorSearchGrid.cshtml"></partial>
    </div>
    <div class="col-9 element-details" id="WorkOrderDetails">
      <div class="row justify-content-center plug">
        <div class="col-6 align-self-center">
          <p class="mt-5 plug-info">@VM_Resources.INFO_SelectPlugInfo</p>
        </div>
      </div>
    </div>
  </div>
</div>

<script src="~/bundles/js/Module/PerformanceWorkOrderMonitor.js"></script>
