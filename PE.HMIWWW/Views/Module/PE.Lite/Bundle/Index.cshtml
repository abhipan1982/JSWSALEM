@model PE.HMIWWW.ViewModel.Module.Lite.Material.VM_RawMaterialOverview
@{
  Layout = "~/Views/Shared/_LayoutWithoutWidgets.cshtml";
}

@section module_menu
{
  @Html.ImageButton("defects", "_RawMaterial_Defect.AssignDefectsPopup()", false, "NAME_AddDefects")
}

<div class="container-fluid p-3">
  <div class="row">
    <div class="col-3 grid-filter" id="RawMaterialSearchGrid">
      @await Html.PartialAsync("~/Views/Module/PE.Lite/Bundle/_BundleSearchGrid.cshtml", Model)
    </div>
    <div class="col-9 element-details" id="RawMaterialDetails">
      <div class="row justify-content-center plug">
        <div class="col-6 align-self-center">
          <img src="~/css/Shared/plug.png" />
          <p class="mt-5 plug-info">@VM_Resources.INFO_SelectPlugInfo</p>
        </div>
      </div>
    </div>
  </div>
</div>

<script src="~/bundles/js/Module/Bundle.js"></script>
<link rel="stylesheet" href="~/bundles/css/Module/Bundle.css" />
