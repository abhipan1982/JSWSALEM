{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "microsoft-signalr@5.0.7",
      "destination": "wwwroot/lib/microsoft-signalr/",
      "files": [
        "signalr.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/",
      "files": [
        "dist/jquery.unobtrusive-ajax.min.js"
      ]
    },
    {
      "library": "jszip@3.7.1",
      "destination": "wwwroot/lib/jszip/",
      "files": [
        "jszip.min.js"
      ]
    }
  ]
}